algoritmo "semnome"
// Função :
// Autor :
// Data : 21/04/2022
// Seção de Declarações
var
n1, n2, c: inteiro
inicio
escreva("Numero Inicial: ")
leia(n1)
escreva("Numero Final: ")
leia(n2)
se(n1 <= n2) entao
   enquanto (n1 <= n2) faca
      escreva(n1)
      n1 <- n1 + 1
   fimenquanto
senao
   enquanto (n1 >= n2) faca
      escreva(n1)
      n1 <- n1 - 1

   fimenquanto
fimse
fimalgoritmo

